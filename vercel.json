{
    "routes": [
      {
        "src": "/(search_song|search_songs|generate_suggestions|playlists|generate_playlist|import_to_existing_playlist|success)",
        "dest": "api/main.py"
      },
      {
        "src": "/",
        "dest": "api/main.py"
      }
    ],
    "functions": {
      "api/main.py": {
        "runtime": "python3.9",
        "maxDuration": 10,
        "includeFiles": "templates/**"
      }
    }
  }
  